{
  "name": "rpodwika/designpatterns",
  "description": "PHP Design Patterns",
  "minimum-stability": "stable",
  "license": "GPL",
  "authors": [
    {
      "name": "Robert Podwika",
      "email": "hello@rpodwika.pl"
    }
  ],
  "require": {
    "phpunit/phpunit": "^4.7",
    "satooshi/php-coveralls": "dev-master",
    "sebastianbergmann/phpcov": "1.1.0"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "sebastianbergmann/phpcov",
        "version": "1.1.0",
        "dist": {
          "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/sebastianbergmann/phpcov.git",
          "type": "git",
          "reference": "1.1.0"
        },
        "bin": [
          "phpcov.php"
        ]
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "Rpodwika\\Designpatterns\\": "src/",
      "Rpodwika\\Tests\\Designpatterns\\": "tests/"
    }
  }
}
